# .gitignore for Next.js

# Ignore environment variable files
.env*

# Ignore Next.js build output
.next/

# Ignore serverless build output
.serverless/

# Ignore coverage reports
coverage/

# Ignore npm-debug.log and yarn-debug.log files
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore .env files containing environment-specific settings
.env.local
.env.development.local
.env.test.local
.env.production.local

# Ignore .DS_Store files
.DS_Store

# Ignore node_modules directory
node_modules/

# Ignore yarn.lock file
yarn.lock
